// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Message {
  sender    String
  receiver  String
  message   String
  time      String
  id        Int @id @default(autoincrement())
}

model User {
  id        Int @id @default(autoincrement())
  name      String
}
